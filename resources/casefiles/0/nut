FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.00001;

boundaryField
{
    inlet
    {
        type            calculated;
        value           $internalField;
    }
    outlet
    {
        type            calculated;
        value           $internalField;
    }
    blade_wall
    {
        type            nutLowReWallFunction ;
        value           uniform 0;
    }
    hub_periodic
    {
        type            zeroGradient ;
    }

    shroud_periodic
    {
        type            zeroGradient ;
    }

    ylower
    {
        type            cyclic;
    }

    yupper
    {
        type            cyclic;
    }
}
// ************************************************************************* //
